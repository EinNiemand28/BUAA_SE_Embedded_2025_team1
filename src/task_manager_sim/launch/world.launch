<launch>

    <!-- World -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find gazebosim_demo)/worlds/library.xml" />
        <arg name="paused" value="false" />
        <arg name="use_sim_time" value="true" />
        <arg name="gui" value="true" />
        <arg name="recording" value="false" />
        <arg name="debug" value="false" />
    </include>

    <!-- Spawn objects -->
    <!-- book -->
    <node name="book" pkg="gazebo_ros" type="spawn_model" args="-file $(find gazebosim_demo)/models/book/model.sdf -x 4.8 -y 2 -z 1.0 -R 1.57 -sdf -model book" />

</launch>